{
  "name": "redbitcz/subreg-api-php",
  "description": "Subreg.cz API client for PHP",
  "keywords": ["subreg", "gransy", "api", "client"],
  "license": ["MIT"],
  "type": "library",
  "authors": [
    {
      "name": "Redbit s.r.o.",
      "homepage": "https://www.redbit.cz"
    },
    {
      "name": "Jakub BouÄek",
      "email": "pan@jakubboucek.cz"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-soap": "*",
    "ext-json": "*",
    "psr/log": "^1.1",
    "nette/schema": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Redbitcz\\SubregApi\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.7",
    "nette/tester": "^2.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Redbitcz\\SubregApiTests\\": "tests/"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src --level 5",
    "tests": "tester tests/"
  }
}
